{
  "name": "coodesh-challenge-mkvlrn",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Mike Valeriano <mkvlrn@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "src/*"
  ],
  "scripts": {
    "prebackend:build": "npx rimraf build",
    "build": "yarn backend:build && yarn frontend:build",
    "backend:build": "npx tsc -p src/backend/tsconfig.build.json && npx tsc-alias -p src/backend/tsconfig.build.json",
    "backend:dev": "dotenv -e .env -- npx nodemon --ext ts --ignore '**/*.spec.ts' --ignore 'src/frontend' --exec ts-node src/backend/index.ts",
    "frontend:build": "NODE_ENV=production webpack --no-stats && npx tsc -p src/frontend/tsconfig.build.json && npx tsc-alias -p src/frontend/tsconfig.build.json",
    "frontend:dev": "NODE_ENV=development webpack serve --no-stats",
    "test": "jest",
    "prepare": "npx husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
